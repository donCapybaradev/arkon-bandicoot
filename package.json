{
	"name": "arkon-bandicoot",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-scripts": "5.0.1",
		"web-vitals": "^2.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@rsbuild/core": "^1.6.4",
		"@rsbuild/plugin-react": "^1.4.2",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/node": "^20.0.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"husky": "^9.1.7",
		"jsdom": "^24.0.0",
		"lint-staged": "^16.2.6",
		"typescript": "^5.6.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^2.0.0"
	},
	"scripts": {
		"build": "bunx rsbuild build",
		"dev": "bunx rsbuild dev",
		"fix": "bunx biome check src --write",
		"prepare": "husky",
		"test:ci": "bunx vitest run --reporter=dot",
		"test": "bunx vitest"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx,json,css,md}": [
			"bunx biome check --write"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
